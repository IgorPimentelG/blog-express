<h2>Lista de categorias:</h2>
<hr />
<a href="/v1/admin/categories/add">
  <button class="btn btn-success">
    Nova Categoria
  </button>
</a>

{{#each categories}}
<div class="card mt-4">
  <div class="card-body">
    <h4>{{ name }}</h4>
    <small>{{ slug }}</small>
    <br />
    <a href="/v1/admin/categories/edit/{{_id}}">
      <button class="btn btn-success mt-4">Editar</button>
    </a>
  </div>
</div>
{{else}}

{{/each}}